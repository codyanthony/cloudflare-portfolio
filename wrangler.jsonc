{
	"main": "dist/_worker.js/index.js",
	"name": "virtual-visual",
  "compatibility_date": "2025-12-14",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
    "enabled": true
  },
  "kv_namespaces": [
    {
      "binding": "VISITOR_COUNT",
      "id": "YOUR_KV_NAMESPACE_ID_HERE"
    }
  ]
}
